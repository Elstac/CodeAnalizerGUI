<UserControl x:Class="CodeAnalizerGUI.UserControls.MainWindowControls.Views.FileExplorerControl"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:CodeAnalizerGUI.UserControls.MainWindowControls.Views"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800">
    <StackPanel>
        <TreeView ItemsSource="{Binding TreeItems}"/>
        <TextBlock Text="{Binding selectedItem}" />
        <Button Content="SelectFolder" Command="{Binding SelectCommand}"/>
        <Button Content="Exit" Command="{Binding ExitCommand}"/>
    </StackPanel>
</UserControl>
