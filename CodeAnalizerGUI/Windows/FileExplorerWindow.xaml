<Window x:Class="CodeAnalizerGUI.FileExplorerWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CodeAnalizerGUI"
        mc:Ignorable="d"
        Title="FileExplorerWindow" Height="800" Width="450">
    <Grid>
        <StackPanel>
            <TreeView Name="fileTree"/>
            <TextBlock Name="TestBlock" Text="No file has been chosen"/>

            <Button Content="SelectFolder" Click="Button_Click_1"/>
            <Button Content="Exit" Click="Button_Click"/>
            
        </StackPanel>
        
    </Grid>
</Window>
